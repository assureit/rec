extends base

block content
        table(border=2)
            tr
                th rawID
                th watchID
                th timestamp
                th data
                th priority
            for data,index in item_list
                -var className='list_odd';
                if(index % 2) == 0
                    -className='list_even';
                else
                tr(class='#{className}')
                    td #{data.rawID}
                    td(align='center') #{data.watchID}
                    td #{data.timestamp}
                    td #{JSON.stringify(data.data)}
                    td(align='center') #{data.priority}
  