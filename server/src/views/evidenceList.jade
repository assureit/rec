extends base

block content
        table(border=2)
            tr
                th evidenceID
                th nodeID
                th timestamp
                th status
            for data,index in item_list
                -var className='list_odd';
                if(index % 2) == 0
                    -className='list_even';
                else
                tr(class='#{className}')
                    td(align='center') #{data.evidenceID}
                    td(align='center') #{data.nodeID}
                    td #{data.timestamp}
                    td(align='center') #{data.status}
  