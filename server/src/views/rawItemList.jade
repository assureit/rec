extends base

block content
        table(border=2)
            tr
                th watchID
                th name
                th priority
            for data,index in item_list
                -var className='list_odd';
                if(index % 2) == 0
                    -className='list_even';
                else
                tr(class='#{className}')
                    td(align='center') #{data.watchID}
                    td #{data.name}
                    td #{data.datatype}
  