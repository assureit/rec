extends base

block content
        table(border=2)
            tr
                th presetID
                th description
                th datatype
                th paramName
                th paramType
                th paramDesc
            for data,index in item_list
                -var className='list_odd';
                if(index % 2) == 0
                    -className='list_even';
                else
                tr(class='#{className}')
                    td(align='center') #{data.presetID}
                    td #{data.description}
                    td #{data.datatype}
                    td #{data.paramName}
                    td #{data.paramType}
                    td #{data.paramDesc}
  